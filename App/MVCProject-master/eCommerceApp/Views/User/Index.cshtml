@model ICollection<eCommerceApp.Models.ApplicationUser>
@{
    ViewData["Title"] = "Index";
}

<h1>All Users</h1>

<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">User ID</th>
      <th scope="col">User Email</th>
      <th scope="col">UserName</th>
      
      @*<th scope="col">Edit</th>*@
      <th scope="col">Delete</th>
    </tr>
  </thead>
  <tbody>
      @foreach (var item in Model)
      {
          <tr>
              <td>@item.Id</td>
              <td>@item.Email</td>
              <td>@item.UserName</td>
              
              @*<td><a class="btn btn-primary" asp-area="" asp-controller="user" asp-action="edit" asp-route-id="@item.Id">Edit</a></td>*@
              <td><a class="btn btn-danger" href="/user/delete/@item.Id">Delete</a></td>
          </tr>
      }
    
    
  </tbody>
</table>
